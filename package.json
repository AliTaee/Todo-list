{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "Simple todo list with React, Redux and Next.js",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "e2e": "cypress open",
    "unit": "jest __test__/",
    "unit:coverage": "jest __test__/  --coverage",
    "unit:watch": "jest __test__/ --watch",
    "lint": "eslint .babelrc.js --no-ignore env-config.js pages/**/*.js pages/**/*.jsx src/**/*.jsx src/**/*.js static/**/*.js next.config.js pwabuilder-sw.js",
    "format": "prettier --check src/**/*.js pages/**/*.js src/**/*.jsx pages/**/*.jsx",
    "export": "next export",
    "deploy": "npm run build && npm run export"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "Todolist",
    "React",
    "Next.js",
    "Redux",
    "Jest"
  ],
  "author": "Ali Taee",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "@zeit/next-sass": "^1.0.1",
    "css-reset-and-normalize": "^2.1.0",
    "dotenv": "^8.1.0",
    "next": "^11.1.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-define": "^1.3.1",
    "cypress": "^4.1.0",
    "dev": "^0.1.3",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-testing-library": "^3.1.3",
    "eslint-utils": ">=1.4.1",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0",
    "redux-mock-store": "^1.5.4"
  }
}
